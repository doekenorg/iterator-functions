{
    "name": "doekenorg/iterator-functions",
    "description": "A package that provides array_* like functions for iterators.",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.17",
        "vimeo/psalm": "^4.9",
        "phpbench/phpbench": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "DoekeNorg\\IteratorFunctions\\": "src"
        },
        "files": [
            "src/iterator_functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "DoekeNorg\\IteratorFunctions\\Tests\\": "tests"
        }
    },
