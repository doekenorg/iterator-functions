{
    "name": "doekenorg/iterator-functions",
    "description": "A package that provides array_* like functions for iterators.",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.15",
        "vimeo/psalm": "^4.9"
    },
    "autoload": {
        "psr-4": {
            "DoekeNorg\\IteratorFunctions\\": "src"
        },
        "files": [
            "src/iterator_functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "DoekeNorg\\IteratorFunctions\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Doeke Norg",
            "email": "doekenorg@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
